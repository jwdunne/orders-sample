{
    "compilerOptions": {
        "target": "ES2022",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "strict": true,
        "declaration": true,
        "skipLibCheck": true,
        "noImplicitReturns": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "outDir": "dist",
    },
    "exclude": [
        "node_modules",
        "dist",
        "cdk.out"
    ]
}
